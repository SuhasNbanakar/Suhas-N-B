package com.eclipse.imp;

public class StringMethodFour {

	public static void main(String[] args) {

		String book="  Good book Read Once Again ";
		
		String boo=book.substring(2,20);// op= od book Read Once
		System.out.println("running sub string = "+boo);
		
		String boo1=book.toString();//op= Good book Read Once Again
		System.out.println("running to string = "+boo1);
		
		String boo2=book.trim();// starting and end blank space is trim  
		System.out.println("running trim = "+boo2);
		
		String boo3=String.valueOf(book);
		System.out.println("running valueof = "+boo3);
		 
		// String	valueOf(char c)
		 
		// String	valueOf(double d)
		
		// String	valueOf(float f)
		 
		 //String	valueOf(int i)
		
		 //String	valueOf(long l)
		 
		// String	valueOf(Object obj)
	}

}
